<?xml version="1.0" encoding="UTF-8"?> 
<ISUI>
	<Window name='EvEbot'> 
		<Title>EVEBot</Title>
		<Visible>1</Visible>
		<X>250</X> 
		<Y>200</Y> 
		<Width>417</Width> 
		<Height>300</Height>
		<TitleBar>		
			<Height>16</Height>
			<Border>3</Border>
			<BorderColor>FF333333</BorderColor>
			<BackgroundColor>FF555555</BackgroundColor>
			<Width>100%</Width>
      			<Children>
				<text name='TitleName' >
				<X>2</X>
				<Y>2</Y>
				<Width>100%</Width>
				<Height>100%</Height>
				<text>		EVEBot v.${Script[evebot].VariableScope.Version}</text>
				</text>
				<button Name='Close' template='eve.window.TitleBar.Close'>
					<OnLeftClick>
						endscript evebot
					</OnLeftClick>
				</button>
				<button Name='Minimize' template='eve.window.TitleBar.Minimize'>
					<OnLeftClick>
						UIElement[eveBot].FindUsableChild[TitleBotStatus,text]:Show
						UIElement[eveBot].FindUsableChild[TitleBotStatusCap,text]:Show
						UIElement[eveBot].FindUsableChild[TitleBotXPStatus,text]:Show
						UIElement[eveBot].FindUsableChild[TitleBotXPCap,text]:Show
					</OnLeftClick>
				</button>
				<button Name='Maximize' template='eve.window.TitleBar.Maximize'>
					<OnLeftClick>
						UIElement[eveBot].FindUsableChild[TitleBotStatus,text]:Hide
						UIElement[eveBot].FindUsableChild[TitleBotXPStatus,text]:Hide
						UIElement[eveBot].FindUsableChild[TitleBotXPCap,text]:Hide
						UIElement[eveBot].FindUsableChild[TitleBotStatusCap,text]:Hide
					</OnLeftClick>
				</button>

			</Children>
		</TitleBar>

		<Children Name='EvebotChild'>
			<TabControl Name='EvEBotTab'>
				<X>0</X>
				<Y>0</Y>
				<Width>100%</Width>
				<Height>100%</Height>
				<FrameTexture />
				<Tabs>
					<Tab Name='Main'>
						<commandbutton name='Pause EvEBot'> 
							<X>275</X> 
							<Y>5</Y>
							<Width>65</Width>
							<Height>25</Height>
							<Text>Pause</Text>
							<OnLeftClick>
								EVE:Execute[CmdStopShip]
								Script[EvEBot]:Pause
								Echo Paused
							</OnLeftClick>
						</commandbutton>
						<commandbutton name='Play EvEBot'> 
							<X>205</X> 
							<Y>5</Y>
							<Width>65</Width>
							<Height>25</Height>
							<Text>Play</Text>
							<OnLeftClick>
								Script[EvEBot]:Resume
								Echo Resume
							</OnLeftClick>
						</commandbutton>
						<commandbutton name='Exit EvEBot'> 
							<X>345</X> 
							<Y>5</Y>
							<Width>65</Width>
							<Height>25</Height>
							<Text>Exit</Text>
							<OnLeftClick>
								endscript evebot
							</OnLeftClick>
						</commandbutton>
						<text name='Running Time is'>
							<X>7</X> 
							<Y>5</Y> 
							<Width>100</Width> 
							<Height>20</Height>
							<Text>Time: </Text>
						</text>
						<Text name='EvE Bot Running Time'> 
							<X>50</X> 
							<Y>5</Y> 
							<Width>100</Width> 
							<Height>20</Height> 
							<Text>${Math.Calc[(${Script[EvEBot].RunningTime}/1000/60/60)%60].Int.LeadingZeroes[2]}:${Math.Calc[(${Script[EvEBot].RunningTime}/1000/60)%60].Int.LeadingZeroes[2]}:${Math.Calc[(${Script[EvEBot].RunningTime}/1000)%60].Int.LeadingZeroes[2]}</Text> 
						</Text>
						<text name='Status'>
							<X>7</X> 
							<Y>20</Y> 
							<Width>100</Width> 
							<Height>20</Height>
							<Text>State:</Text>
						</text>
						<text name='Status Code'>
							<X>55</X> 
							<Y>20</Y> 
							<Width>100</Width> 
							<Height>20</Height>
							<text>${Script[evebot].VariableScope.botstate}</text>
						</text>
						<console Name="EVEStatus">
							<Y>r100</Y>
							<Width>100%</Width> 
							<Height>100</Height> 
						</console>
					</Tab>
					<Tab Name='Mining'>
