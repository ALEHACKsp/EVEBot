<?xml	version="1.0"	encoding="UTF-8"?>
<ISUI>
	<Window	name='EVEBot'>
		<BackgroundColor></BackgroundColor>
		<Border>0</Border>
		<Title>EVEBot</Title>
		<Visible>1</Visible>
		<X>250</X>
		<Y>200</Y>
		<Width>169</Width>
		<Height>133</Height>
		<TitleBar template='EVESkin.Window.TitleBar'>
			<Width>100%</Width>
			<Height>20</Height>
			<Border>0</Border>
			<Children>
				<commandbutton name='Start EVEBot' template='EVESkin.Window.TitleBar.PlayButton'>
					<X>5</X>
					<Y>4</Y>
					<Width>46</Width>
					<Height>16</Height>
					<OnLeftClick>
						Script[EVEBot]:Resume
						Echo EVEBot: Resume
					</OnLeftClick>
				</commandbutton>
				<commandbutton name='Pause EVEBot' template='EVESkin.Window.TitleBar.PauseButton'>
					<X>51</X>
					<Y>4</Y>
					<Width>46</Width>
					<Height>16</Height>
					<OnLeftClick>
						EVE:Execute[CmdStopShip]
						Script[EVEBot]:Pause
						Echo EVEBot: Paused
					</OnLeftClick>
				</commandbutton>
				<button	Name='Minimize'	template='EVESkin.Window.TitleBar.Minimize'>
					<OnLeftClick>
						UIElement[EVEBot].FindUsableChild[TitleBotStatus,text]:Show
						UIElement[EVEBot].FindUsableChild[TitleBotStatusCap,text]:Show
						UIElement[EVEBot].FindUsableChild[TitleBotXPStatus,text]:Show
						UIElement[EVEBot].FindUsableChild[TitleBotXPCap,text]:Show
					</OnLeftClick>
				</button>
				<button	Name='Maximize'	template='EVESkin.Window.TitleBar.Maximize'>
					<OnLeftClick>
						UIElement[EVEBot].FindUsableChild[TitleBotStatus,text]:Hide
						UIElement[EVEBot].FindUsableChild[TitleBotXPStatus,text]:Hide
						UIElement[EVEBot].FindUsableChild[TitleBotXPCap,text]:Hide
						UIElement[EVEBot].FindUsableChild[TitleBotStatusCap,text]:Hide
					</OnLeftClick>
				</button>
				<button	Name='Close' template='EVESkin.Window.TitleBar.Close'>
					<OnLeftClick>
						endscript EVEBot
					</OnLeftClick>
				</button>
			</Children>
		</TitleBar>
		
		<Children>
			<frame name='EVEBot_bottom' template='EVESkin.Window.Main'>
				<X>0</X>
				<Y>4</Y>
				<Height>113</Height>
				<Width>100%</Width>
				<Border>0</Border>
				<BackgroundColor></BackgroundColor>
				<Children>
					<text name='TitleName' >
					<X>15</X>
					<Y>8</Y>
					<Width>100%</Width>
					<Height>67</Height>
					<text>EVEBot v.${Script[EVEBot].VariableScope.Version}</text>
					</text>
					<text name='script_runtime' template='EVESkin.Font.Description'>
						<X>25</X>
						<Y>30</Y>
						<Width>115</Width>
						<Height>20</Height>
						<Text>Script Runtime:</Text>
					</text>
					<Text name='script_runtime_data' template='EVESkin.Font.Value'>
						<X>90</X>
						<Y>30</Y>
						<Width>100</Width>
						<Height>20</Height>	
						<Text>${Script[EVEBot].VariableScope.UI.Runtime}</Text>
					</Text>

					<text name='state' template='EVESkin.Font.Description'>
						<X>25</X>
						<Y>45</Y>
						<Width>100</Width>
						<Height>20</Height>
						<Text>State:</Text>
					</text>
					<text name='state_data' template='EVESkin.Font.Value'>
						<X>90</X>
						<Y>45</Y>
						<Width>100</Width>
						<Height>20</Height>
						<text>${Script[EVEBot].VariableScope.botstate}</text>
					</text>

					<button name='EVEBot Options' template='EVESkin.Window.Main.OptionsButton'>
						<X>55</X>
						<Y>84</Y>
						<Width>59</Width>
						<Height>16</Height>
						<onleftclick>UIElement -toggle EVEBotOptionsUI</onleftclick>
					</button>
				</Children>
			</frame>
		</Children>
	</Window>

	<window name='EVEBotOptionsUI'>
		<Visible>0</Visible>
		<BackgroundColor></BackgroundColor>
		<Border>0</Border>
		<X>200</X>
		<Y>200</Y>
		<Width>500</Width>
		<Height>326</Height>
		<Title>EVEBot Options</Title>
		<TitleBar template='EVESkin.Window.EVEBotOptionsUI.TitleBar'>
			<Width>100%</Width>
			<Height>16</Height>
			<Border>0</Border>
			<Children>
				<text name='EVEBotOptionsUI_TitleBar_Title' template='EVESkin.Font.Description'>
				<X>210</X>
				<Y>3</Y>
				<Width>100</Width>
				<Height>20</Height>
				<Text>EVEBot Options</Text>
				</text>				
			</Children>
		</TitleBar>
		<Children>
			<TabControl	Name='EVEBotOptionsTab' template='EVESkin.EVEBotOptionsUI.TabControl'>
				<Draggable>false</Draggable>
				<BackgroundColor></BackgroundColor>
				<Border>0</Border>
				<X>0</X>
				<Y>10</Y>
				<Width>100%</Width>
				<Height>100%</Height>
				<Tabs>
					<Tab Name='Status'>
							<text	name='Trips'>
								<X>205</X>
								<Y>75</Y>
								<Width>100</Width>
								<Height>20</Height>
								<Text>Runs:</Text>
							</text>
							<text	name='Trips_Data'>
								<X>245</X>
								<Y>75</Y>
								<Width>100</Width>
								<Height>20</Height>
								<text>${Script[EVEBot].VariableScope.Miner.TotalTrips}</text>
							</text>
							<text	name='avg_time'>
								<X>7</X>
								<Y>30</Y>
								<Width>175</Width>
								<Height>20</Height>
								<Text>Time For Current Trip:</Text>
							</text>
							<text	name='avg_time_data'>
								<X>10</X>
								<Y>45</Y>
								<Width>100</Width>
								<Height>20</Height>
								<text>${Script[EVEBot].VariableScope.Miner.TripDuration} Sec</text>
							</text>
							<text	name='Average Time/Trip'>
								<X>7</X>
								<Y>60</Y>
								<Width>175</Width>
								<Height>20</Height>
								<Text>Average Time per Trip:</Text>
							</text>
							<text	name='Average Time/Trip Data'>
								<X>10</X>
								<Y>75</Y>
								<Width>150</Width>
								<Height>20</Height>
								<text>${Script[EVEBot].VariableScope.Miner.AverageTripSeconds}</text>
							</text>						
								<console Name="StatusConsole">
								<Y>r100</Y>
								<Width>100%</Width>
								<Height>100</Height>
							</console>		
					</Tab>
					
					<Tab Name='Main'>
					</Tab>
    	
					<Tab name='Mining'>
					</tab>
    	
					<Tab name='Combat'>
						  <Text name='MinShield'>
									<X>40</X>
									<Y>10</Y>
									<Width>140</Width>
									<Height>10</Height>
									<Text>Minimum Shield</Text>
								</Text>
							 <Textentry name='MinShieldPctInput'>		
									<X>6</X>							
									<Y>10</Y>						
									<Width>32</Width>						
									<Height>18</Height>
									<OnChange>
									This:SetText[${MinShieldPct}]
									</OnChange>				
								</Textentry>
							  <Text name='MinStructure'>
									<X>40</X>
									<Y>30</Y>
									<Width>140</Width>
									<Height>10</Height>
									<Text>Minimum Structure</Text>
								</Text>
							 <Textentry name='MinStructurePctInput'>		
									<X>6</X>							
									<Y>30</Y>						
									<Width>32</Width>						
									<Height>18</Height>
									<OnChange>
									This:SetText[${MinStructurePct}]
									</OnChange>				
								</Textentry>
					</tab>
    	
					<Tab name='Market'>
					</tab>
					
					<tab name='Fitting'>
					</tab>
					
					<tab name='Skills'>
						<commandbutton name='queueskill'>
							<X>210</X>
							<Y>5</Y>
							<Width>97</Width>
							<Height>25</Height>
							<text>Queue</text>
							<Command></Command>
						</commandbutton>
						<commandbutton name='removeskill'>
							<X>313</X>
							<Y>5</Y>
							<Width>97</Width>
							<Height>25</Height>
							<text>Remove</text>
							<Command></Command>
						</commandbutton>
						<listbox name='skills'>
							<X>5</X>
							<Y>5</Y>
							<Width>200</Width>
							<Height>258</Height>
							<Items>
							AddItem[${Me.Skill[1]}]
    	
							</Items>
						</listbox>
						<listbox name='intraining'>
							<X>210</X>
							<Y>40</Y>
							<Width>200</Width>
							<Height>223</Height>
							<Items>
							</Items>
						</listbox>
					</tab>
					<Tab Name='About'>
					<Text	name='developers1'>
						<X>5</X>
						<Y>145</Y>
						<Width>400</Width>
						<Height>10</Height>
						<Text>EVEBot was created by:</Text>
					</Text>
					<Text	name='developers2'>
						<X>5</X>
						<Y>155</Y>
						<Width>400</Width>
						<Height>10</Height>
						<Text>Neutro, Tehtsuo, thehazard, Stealthy,	Hessinger, Altered</Text>
					</Text>
    	
					</Tab>
				</Tabs>
			</tabcontrol>
		</Children>
	</window> 
</ISUI>